{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "moduleResolution": "node16",
    "module": "node16",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [".", "**/*.js", "**/*.d.ts"],
  "exclude": ["node_modules"]
}
